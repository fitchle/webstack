{
  "name": "@stackfitch/db",
  "version": "0.1.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "setup": "edgedb project init && edgedb configure set listen_addresses 127.0.0.1 ::1 && npx @edgedb/generate edgeql-js",
    "generate": "npx @edgedb/generate edgeql-js",
    "migrate": "edgedb migrate && pnpm generate",
    "push": "edgedb migration create && pnpm migrate"
  },
  "devDependencies": {
    "@edgedb/generate": "^0.3.0",
    "@stackfitch/tsconfig": "workspace:*",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "edgedb": "^1.3.2"
  }
}
